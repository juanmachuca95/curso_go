}

#footer .icons .rounded {
  border-color: #3e3e3e;
  font-size: 1.5em;
}
#footer .icons a { color:#3e3e3e !important;}



#footer .icons .rounded:active, #footer .icons .rounded:hover {
  border-color: #ffffff;
