  position: relative;
  width: 150%;
}

ul.tabular li:first-child {
  border-top: 0;
  margin-top: 0;
}

ul.tabular li h3 {
  left: 0px;
  position: absolute;
  text-align: center;
  top: 12px;
  vertical-align: top;

}
#footer .container {padding-left: 0%;}


